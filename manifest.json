{
  "name": "Dark Everything",
  "description": "Every website will be in dark mode.",
  "version": "1.1",
  "manifest_version": 3,
  "permissions": [
    "bookmarks"
  ],
  "action": {
    "default_title": "Dark Everything",
    "default_icon": "icon.png",
    "default_popup": "dark.html"
  },
  "content_scripts": [{
    "js": ["dark.js"],
    "css": ["dark.css"],
    "exclude_matches": ["http://localhost:*/*", "http://localhost:*/*/*", "http://localhost:*/*/*/*", "*://www.youtube.com/*", "*://youtube.com/","*://youtube.com/*/*", "*://npmjs.com/", "*://npmjs.com/*/*", "*://fsharetv.co/*", "*://kissasian.li/", "*://kissasian.la/", "*://kissasian.li/*", "https://kissasian.li/*/*/*", "https://hindilinks4u.pro/", "https://hindilinks4u.pro/*"],
    "matches": ["*://*/", "*://*/*", "https://*.google.com/*"]
  }],
  "key": ""
}
